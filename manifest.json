{
  "name": "Annual Snowfall Data",
  "description": "This chrome packaged app visualizes annual snowfall data",
  "version": "1.0",
  "manifest_version": 2,	
  "permissions":[
      "https://maps.google.com/*",
      "https://maps.googleapis.com/*"
  ],
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "sandbox": {
    "pages" : ["map.html", "chart.html"]
    
  }

}